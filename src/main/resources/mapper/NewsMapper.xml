<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.echo.country.mapper.NewsMapper">

    <resultMap id="BaseResultMap" type="com.echo.country.pojo.News">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="ico" column="ico" jdbcType="VARCHAR"/>
        <result property="title" column="title" jdbcType="VARCHAR"/>
        <result property="descb" column="descb" jdbcType="VARCHAR"/>
        <result property="recever" column="recever" jdbcType="VARCHAR"/>
        <result property="institutions" column="institutions" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
        <result property="isdeleted" column="isdeleted" jdbcType="INTEGER"/>
        <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,ico,title,
        descb,recever,institutions,
        create_time,isdeleted,update_time
    </sql>
</mapper>
